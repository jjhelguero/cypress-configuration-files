{
  "automerge": true,
  "extends": ["config:base"],
  "labels": ["type: dependencies", "renovate"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "excludePackagePatterns": [
        "cypress",
        "cypress-grep"
      ],
      "enabled": false
    }
  ],
  "prHourlyLimit": 2,
  "schedule": [
      "after 10pm and before 5am on every weekday",
      "every weekend"
    ],
  "timezone": "America/Chicago",
  "updateNotScheduled": false
}